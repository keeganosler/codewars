package main

import "fmt"

func main() {
    var strarr = []string {"zone", "abigail", "theta", "form", "libe", "zas", "theta", "abigail"}
    var res int
    res = longest_consec(strarr, 2)
    fmt.Printf(string(res))
}

func longest_consec(arr []string, len int) int{
    var maxString = 0
    for i:=0; i<7; i++ {
        maxString = maxString + 1
    }
    return maxString
}
